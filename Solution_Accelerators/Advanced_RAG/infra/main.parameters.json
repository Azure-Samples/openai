    "aksUserPoolVMSize": {
      "value": "standard_a2m_v2"
    },
    "virtualNetworkAddressPrefix": {
      "value": "10.255.0.0/16"
    },
    "subnetPrefixes": {
      "value": {
        "aksSubnetPrefix": "10.255.1.0/24",
        "endpointsSubnetPrefix": "10.255.101.0/24",
        "appGatewaySubnetPrefix": "10.255.201.0/24"
      }
    },
    "tag": {
      "value": {
        "Purpose": "For developing multimodal bot PoC"
      }
    }
