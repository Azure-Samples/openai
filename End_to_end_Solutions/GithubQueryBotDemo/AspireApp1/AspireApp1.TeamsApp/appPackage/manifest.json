{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
    "manifestVersion": "1.16",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.microsoft.teams.extension",
  "developer": {
    "name": "Teams App, Inc.",
    "websiteUrl": "https://github.com/dotnet/runtime/",
    "privacyUrl": "https://github.com/dotnet/runtime/blob/main/LICENSE.TXT",
    "termsOfUseUrl": "https://github.com/dotnet/runtime/blob/main/CODE-OF-CONDUCT.md"
  },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "dotnet/runtime Teams App",
        "full": "Teams Bot for dotnet/runtime"
    },
    "description": {
        "short": "Teams Bot for dotnet/runtime",
        "full": "This Teams Bot helps query dotnet/runtime repo for top issues that match a certain query."
    },
    "accentColor": "#FFFFFF",
    "bots": [
        {
        "botId": "${{BOT_ID}}",
        "scopes": [
            "personal",
            "team",
            "groupchat"
        ],
        "supportsFiles": false,
        "isNotificationOnly": false
        }
    ],
    "composeExtensions": [
    ],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}